{
  "compilerOptions": {
    "rootDir": "./src",
    "outDir": "./lib",
    "target": "ES2015",
    "lib": [
      "es6",
      "WebWorker"
    ],
    "strict": true,
    "noImplicitAny": true,
    "removeComments": true,
    "preserveConstEnums": true,
    "moduleResolution": "bundler",
    "strictFunctionTypes": false
  }
}
