{
  "openapi": "3.0.0",
  "info": {
    "title": "Object in request",
    "version": "1.0.0"
  },
  "paths": {
    "/additional/properties": {
      "get": {
        "operationId": "AdditionalProperties",
        "requestBody": {
          "content": {
            "application/json": {
              "schema": {
                "$ref": "#/components/schemas/Messages"
              }
            }
          }
        },
        "responses": {
          "200": {
            "description": "Ok",
            "content": {
              "application/json": {
                "schema": {
                  "$ref": "#/components/schemas/Messages"
                }
              }
            }
          },
          "404": {
            "description": "Not found",
            "content": {
              "application/json": {
                "schema": {
                  "additionalProperties": {
                    "type": "object",
                    "properties": {
                      "code": {
                        "type": "integer"
                      },
                      "text": {
                        "type": "string"
                      }
                    }
                  }
                }
              }
            }
          },
          "500": {
            "description": "Error",
            "content": {
              "application/json": {
                "schema": {
                  "additionalProperties": true
                }
              }
            }
          }
        }
      }
    }
  },
  "components": {
    "schemas": {
      "Messages": {
        "type": "object",
        "additionalProperties": {
          "$ref": "#/components/schemas/Message"
        }
      },
      "Message": {
        "type": "object",
        "properties": {
          "code": {
            "type": "integer"
          },
          "text": {
            "type": "string"
          }
        }
      },
      "User": {
        "type": "object",
        "additionalProperties": false,
        "required": [
          "username",
          "email"
        ],
        "properties": {
          "username": {
            "type": "string",
            "example": "johndoe"
          },
          "email": {
            "type": "string",
            "format": "email",
            "example": "john@example.com"
          },
          "age": {
            "type": "integer",
            "minimum": 0,
            "example": 30
          }
        }
      }
    }
  }
}
