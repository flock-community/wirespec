{
  "swagger": "2.0",
  "info": {
    "title": "Object in request",
    "version": "1.0.0"
  },
  "consumes": [
    "application/json"
  ],
  "produces": [
    "application/json"
  ],
  "paths": {
    "/additional/properties": {
      "get": {
        "operationId": "AdditionalProperties",
        "parameters": [
          {
            "in": "body",
            "name": "body",
            "schema": {
              "$ref": "#/definitions/Messages"
            }
          }
        ],
        "responses": {
          "200": {
            "description": "Ok",
            "schema": {
              "$ref": "#/definitions/Messages"
            }
          },
          "404": {
            "description": "Not found",
            "schema": {
              "additionalProperties": {
                "type": "object",
                "properties": {
                  "code": {
                    "type": "integer"
                  },
                  "text": {
                    "type": "string"
                  }
                }
              }
            }
          },
          "500": {
            "description": "Error",
            "schema": {
              "additionalProperties": true
            }
          }
        }
      }
    }
  },
  "definitions": {
    "Messages": {
      "type": "object",
      "additionalProperties": {
        "$ref": "#/definitions/Message"
      }
    },
    "Message": {
      "type": "object",
      "properties": {
        "code": {
          "type": "integer"
        },
        "text": {
          "type": "string"
        }
      }
    },
    "User": {
      "type": "object",
      "additionalProperties": false,
      "required": [
        "username",
        "email"
      ],
      "properties": {
        "username": {
          "type": "string",
          "example": "johndoe"
        },
        "email": {
          "type": "string",
          "format": "email",
          "example": "john@example.com"
        },
        "age": {
          "type": "integer",
          "minimum": 0,
          "example": 30
        }
      }
    }
  }
}
