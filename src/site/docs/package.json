    "@flock/black-sun": "^0.2.0",
    "@flock/wirespec": "file:../../plugin/npm/build/dist/js/productionLibrary/",
    "@mdx-js/react": "^3.0.0",
    "clsx": "^2.0.0",
    "docusaurus-plugin-drawio": "^0.4.0",
